{
  "suggest.noselect": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "vim",
    "rust",
    "typescript",
    "html",
    "html.handlebars",
    "javascript",
    "css",
    "markdown",
    "ruby"
    ],
    "coc.preferences.promptInput": true,
    "coc.preferences.enableFloatHighlight": true,
    "coc.preferences.bracketEnterImprove": true,

    "highlight.colorNames.enable": false,
    "highlight.document.enable": false,

    "emmet.excludeLanguages": [
      "eruby"
    ],

    "solargraph.formatting": true,
    "solargraph.autoformat": true,
    "solargraph.diagnostics": true,

    "rust-analyzer.cargo.allFeatures": true,

    "eslint.run": "onType",
    "eslint.probe": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact",
      "html",
      "vue",
      "markdown"
    ],
    "eslint.autoFixOnSave": true,
    "eslint.format.enable": false,
    "eslint.codeActionsOnSave.mode": "all",
    "eslint.alwaysShowStatus": true,
    "eslint.codeAction.showDocumentation": {
      "enable": true
    },
    "eslint.codeAction.disableRuleComment": {
      "enable": true,
      "location": "separateLine"
    },
    "eslint.workingDirectories": ["."],

    "snippets.ultisnips.directories": [
      "UltiSnips",
      "UnitySnippets"
    ],
    "tsserver.formatOnType": false,
    "prettier.eslintIntegration": false,
    "prettier.formatterPriority": 1000000,

    "stylelintplus.autoFixOnSave": true,
    "stylelintplus.autoFixOnFormat": true,

    "rust-analyzer.procMacro.enable": true,

    "list.source.bibtex.files": ["/Projects/School/Latex/hf/refs.bib"],

    "pairs.disableLanguages": ["brainfuck"]
}
